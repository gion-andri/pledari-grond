<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='de.uni_koeln.spinfo.maalr.user'>
	<!-- Inherit the core Web Toolkit stuff. -->
	<inherits name='de.uni_koeln.spinfo.maalr.common.Common' />
	<inherits name='de.uni_koeln.spinfo.maalr.mongo.Mongo' />
	<inherits name='de.uni_koeln.spinfo.maalr.webapp.ui.common.Common' />
	<inherits name='de.uni_koeln.spinfo.maalr.antlr.Antlr4Maalr' />
	<inherits name='de.uni_koeln.spinfo.maalr.lucene.Lucene' />
	<inherits name='de.uni_koeln.spinfo.maalr.services.user.UserServices'/>

	<!-- Specify the app entry point class. -->
	<entry-point class='de.uni_koeln.spinfo.maalr.webapp.ui.user.client.User' />
	
	<!-- GWT's static string i18n -->
	<inherits name='com.google.gwt.i18n.I18N' />
	<extend-property name="locale" values="st"/>
	<extend-property name="locale" values="de"/>
	<set-property-fallback name="locale" value="st"/> 
	
	<!-- Specify the paths for translatable code -->
	<source path='client' />
	<source path='shared' />

</module>
