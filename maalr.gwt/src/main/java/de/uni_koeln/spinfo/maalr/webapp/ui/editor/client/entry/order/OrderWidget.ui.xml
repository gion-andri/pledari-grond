<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m3="urn:import:de.uni_koeln.spinfo.maalr.webapp.ui.editor.client.entry.filter"
	xmlns:m2="urn:import:de.uni_koeln.spinfo.maalr.webapp.ui.editor.client.entry.list" 
	xmlns:c="urn:import:de.uni_koeln.spinfo.maalr.webapp.ui.common.client" 
	xmlns:q="urn:import:de.uni_koeln.spinfo.maalr.webapp.ui.editor.client.entry.query"
	xmlns:gwtbootstrap="urn:import:com.github.gwtbootstrap.client.ui" xmlns:cellview="urn:import:com.google.gwt.user.cellview.client">
	<g:VerticalPanel width="100%">
		<!-- <gwtbootstrap:PageHeader ui:field="header"></gwtbootstrap:PageHeader>-->
		<gwtbootstrap:FluidContainer>
			<gwtbootstrap:FluidRow>
				<gwtbootstrap:Column size="11">
					<cellview:CellTable ui:field="list" width="100%" height="350px"></cellview:CellTable>
				</gwtbootstrap:Column>
				<gwtbootstrap:Column size="1">
					<g:VerticalPanel>
						<gwtbootstrap:Button ui:field="up" icon="CHEVRON_UP"></gwtbootstrap:Button>
						<gwtbootstrap:Button ui:field="down" icon="CHEVRON_DOWN"></gwtbootstrap:Button>
					</g:VerticalPanel>			
				</gwtbootstrap:Column>
			</gwtbootstrap:FluidRow>
		</gwtbootstrap:FluidContainer>
	</g:VerticalPanel>
</ui:UiBinder> 