<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:s="urn:import:de.uni_koeln.spinfo.maalr.webapp.ui.admin.client.stats"
	xmlns:general="urn:import:de.uni_koeln.spinfo.maalr.webapp.ui.admin.client.general" xmlns:gwtbootstrap="urn:import:com.github.gwtbootstrap.client.ui">
	
	<gwtbootstrap:Well>
		<gwtbootstrap:Legend>CPU</gwtbootstrap:Legend>
				<g:HorizontalPanel width="100%">
					<g:cell width="50%" horizontalAlignment="ALIGN_CENTER">
						<g:VerticalPanel width="100%" height="100%">
							<g:cell horizontalAlignment="ALIGN_CENTER">
								<gwtbootstrap:Label>Load</gwtbootstrap:Label>
							</g:cell>
							<g:cell horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE">
								<general:CpuGauge ui:field="cpuGauge"></general:CpuGauge>
							</g:cell>
						</g:VerticalPanel>
					</g:cell>
					<g:cell width="50%">
						<g:VerticalPanel width="100%" height="100%">
							<g:cell horizontalAlignment="ALIGN_CENTER">
								<gwtbootstrap:Label>Utilization</gwtbootstrap:Label>
							</g:cell>
							<g:cell horizontalAlignment="ALIGN_CENTER">
								<s:CpuProcPie ui:field="cpuPie"></s:CpuProcPie>
							</g:cell>
						</g:VerticalPanel>
					</g:cell>
				</g:HorizontalPanel>
	</gwtbootstrap:Well>


</ui:UiBinder> 