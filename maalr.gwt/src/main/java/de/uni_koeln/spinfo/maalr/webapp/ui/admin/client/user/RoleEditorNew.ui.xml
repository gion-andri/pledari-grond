<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:gwtbootstrap="urn:import:com.github.gwtbootstrap.client.ui">

	<gwtbootstrap:FluidContainer>
		<gwtbootstrap:FluidRow>
			<gwtbootstrap:Column size="10">
				<gwtbootstrap:Well>
				    <g:VerticalPanel>
				    	<g:cell>
				    	    <gwtbootstrap:CellTable ui:field="cellTable"></gwtbootstrap:CellTable>
				    	</g:cell>
				    	<g:cell>
				    	    <gwtbootstrap:SimplePager ui:field="pager"></gwtbootstrap:SimplePager>
				    	</g:cell>
				    </g:VerticalPanel>
				</gwtbootstrap:Well>
			</gwtbootstrap:Column>
			<gwtbootstrap:Column size="2">
				<g:VerticalPanel>
				    <g:cell>
				        <gwtbootstrap:Well>
				            <gwtbootstrap:ButtonGroup>
				                <gwtbootstrap:Button ui:field="createUser" type="INFO" icon="PLUS">
				                    Create User
				                </gwtbootstrap:Button>
				            </gwtbootstrap:ButtonGroup>
				        </gwtbootstrap:Well>
				    </g:cell>
				</g:VerticalPanel>
			</gwtbootstrap:Column>
		</gwtbootstrap:FluidRow>
	</gwtbootstrap:FluidContainer>
	
</ui:UiBinder> 