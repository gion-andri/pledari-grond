<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:s="urn:import:de.uni_koeln.spinfo.maalr.webapp.ui.admin.client.stats" xmlns:gwtbootstrap="urn:import:com.github.gwtbootstrap.client.ui">
	
	<gwtbootstrap:Well>
		<gwtbootstrap:Legend>RAM</gwtbootstrap:Legend>
		<g:HorizontalPanel width="100%">
			<g:cell width="50%" horizontalAlignment="ALIGN_CENTER">
				<g:VerticalPanel width="100%" height="100%">
					<g:cell horizontalAlignment="ALIGN_CENTER">
						<gwtbootstrap:Label>System</gwtbootstrap:Label>
					</g:cell>
					<g:cell horizontalAlignment="ALIGN_CENTER">
						<s:MemPie ui:field="systemMem"></s:MemPie>
					</g:cell>
				</g:VerticalPanel>
			</g:cell>
			<g:cell width="50%" horizontalAlignment="ALIGN_CENTER">
				<g:VerticalPanel width="100%" height="100%">
					<g:cell horizontalAlignment="ALIGN_CENTER">
					<gwtbootstrap:Label>Maalr</gwtbootstrap:Label>
					</g:cell>
					<g:cell horizontalAlignment="ALIGN_CENTER">
						<s:MemPie ui:field="javaMem"></s:MemPie>
					</g:cell>
				</g:VerticalPanel>
			</g:cell>
		</g:HorizontalPanel>
	</gwtbootstrap:Well>
	
</ui:UiBinder> 