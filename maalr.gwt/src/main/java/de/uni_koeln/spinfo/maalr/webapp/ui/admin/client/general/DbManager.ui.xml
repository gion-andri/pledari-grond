<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m1="urn:import:de.uni_koeln.spinfo.maalr.webapp.ui.admin.client.general.dbsettings"
	xmlns:m2="urn:import:de.uni_koeln.spinfo.maalr.webapp.ui.admin.client.general.indexsettings"
	xmlns:m3="urn:import:de.uni_koeln.spinfo.maalr.webapp.ui.admin.client.general.backupsettings"
	xmlns:m4="urn:import:de.uni_koeln.spinfo.maalr.webapp.ui.admin.client.common.help"
	xmlns:s="urn:import:de.uni_koeln.spinfo.maalr.webapp.ui.admin.client.stats"
	xmlns:gwtbootstrap="urn:import:com.github.gwtbootstrap.client.ui">

	<gwtbootstrap:Container ui:field="container">
		<gwtbootstrap:Row>
			<!-- <gwtbootstrap:Column size="12" ui:field="chartsColumn"> -->
			<!-- <g:VerticalPanel width="100%" height="100%" ui:field="charts"> -->
			<!-- </g:VerticalPanel> -->
			<!-- </gwtbootstrap:Column> -->
			<gwtbootstrap:Column size="8" ui:field="settingsColumn">
				<gwtbootstrap:Row>
					<gwtbootstrap:Column size="4">
						<m1:DatabaseSettings ui:field="dbSettings"></m1:DatabaseSettings>
					</gwtbootstrap:Column>
					<gwtbootstrap:Column size="4">
						<m2:IndexSettings ui:field="indexSettings"></m2:IndexSettings>
					</gwtbootstrap:Column>
				</gwtbootstrap:Row>
				<gwtbootstrap:Row>
					<gwtbootstrap:Column size="4">
						<m3:BackupSettings ui:field="backupSettings"></m3:BackupSettings>
					</gwtbootstrap:Column>
					<gwtbootstrap:Column size="4">
						<m4:HelpBox ui:field="helpBox"></m4:HelpBox>
					</gwtbootstrap:Column>
				</gwtbootstrap:Row>
			</gwtbootstrap:Column>
		</gwtbootstrap:Row>
	</gwtbootstrap:Container>
</ui:UiBinder> 