<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'>
	<ui:with field='wrapper' type="de.uni_koeln.spinfo.maalr.webapp.ui.editor.client.entry.list.wrapper.ICellWrapper" />
	<ui:with field='i18n' type='de.uni_koeln.spinfo.maalr.webapp.ui.editor.client.EditorUiBinderMessages' />
	<div style="font-size:90%; width: 100%" class="user-actions">
		<button type="button" class="btn btn-mini btn-primary"
			tabindex="-1" ui:field="edit" style="white-space: nowrap; width: 100%; margin-bottom: 5px;">
			<ui:text from='{i18n.editBtn}' />
		</button>
		<button type="button" class="btn btn-mini btn-danger" tabindex="-1"
			ui:field="delete" style="white-space: nowrap; width: 100%;">
			<ui:text from='{i18n.deleteBtn}' />
		</button>
	</div>
</ui:UiBinder>  

