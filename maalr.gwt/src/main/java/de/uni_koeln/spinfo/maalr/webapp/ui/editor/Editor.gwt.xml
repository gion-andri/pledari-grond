<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='de.uni_koeln.spinfo.maalr.editor'>
	<!-- Inherit the core Web Toolkit stuff. -->

	<!-- <inherits name='de.uni_koeln.spinfo.maalr.webapp.ui.user.User'/> -->
	<inherits name="com.google.gwt.user.User"/>
	<inherits name='de.uni_koeln.spinfo.maalr.common.Common' />
	<inherits name='de.uni_koeln.spinfo.maalr.webapp.ui.common.Common' />
	<inherits name='de.uni_koeln.spinfo.maalr.lucene.Lucene' />
	<inherits name='de.uni_koeln.spinfo.maalr.antlr.Antlr4Maalr' />
	<inherits name='de.uni_koeln.spinfo.maalr.services.editor.EditorServices' />
	<inherits name='de.uni_koeln.spinfo.maalr.services.user.UserServices' />
	<inherits name='com.github.gwtbootstrap.datetimepicker.DatetimepickerNoResources' />
	
	<!-- GWT's static string i18n -->
	<inherits name='com.google.gwt.i18n.I18N' />
	<extend-property name="locale" values="sm, en"/>
	<set-property-fallback name="locale" value="sm"/> 

	<!-- <stylesheet src='../assets/style/editor_style.css' />-->


	<!-- Specify the app entry point class. -->
	<entry-point class='de.uni_koeln.spinfo.maalr.webapp.ui.editor.client.Editor' />

	<!-- Specify the paths for translatable code -->
	<source path='client' />
	<source path='shared' />

</module>
